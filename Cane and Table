<script>
// Script to open and close sidebar
function w3_open() {
  document.getElementById("mySidebar").style.display = "block";
  document.getElementById("myOverlay").style.display = "block";
}
 
function w3_close() {
  document.getElementById("mySidebar").style.display = "none";
  document.getElementById("myOverlay").style.display = "none";
}


</script>
  <script>
  /* This JavaScript is not required to work, but adds functionality for capturing touch events on mobile devices if you want to support mobile */

function handleStart(event) {
  this.className = 'before active';
}

function handleEnd(event) {
  this.className = 'before';
}

var els = document.getElementsByClassName('before');
var n = els.length;

while (n--) {
  els[n].addEventListener('touchstart', handleStart, false);
  els[n].addEventListener('touchend', handleEnd, false);
}
  </script>
